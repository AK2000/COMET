# Sparse matrix dense matrix elementwise subtraction
# Sparse matrix is in CSR format
# RUN: comet-opt --convert-ta-to-it --convert-to-loops %s &> eltwise_sub_CSRxDense.mlir
# RUN: mlir-opt --convert-scf-to-std --convert-std-to-llvm eltwise_sub_CSRxDense.mlir &> eltwise_sub_CSRxDense.llvm
# RUN: export SPARSE_FILE_NAME0=%comet_integration_test_data_dir/test_rank2.mtx
# RUN: mlir-cpu-runner eltwise_sub_CSRxDense.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s

def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [?];
	IndexLabel [j] = [?];        

	#Tensor Declarations
	Tensor<double> A([i, j], {CSR});	  
	Tensor<double> B([i, j], {Dense});
	Tensor<double> C([i, j], {Dense});

    A[i, j] = comet_read(0);

	#Tensor Fill Operation
	B[i, j] = 1.7;
	C[i, j] = 0.0;

	C[i, j] = A[i, j] - B[i, j];
	print(C);
}

